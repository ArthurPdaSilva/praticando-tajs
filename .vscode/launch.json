{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Noje.js Debug Port",
      "request": "attach",
      "type": "node",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/**/node_modules/**/*.js",
        "${workspaceFolder}/**/*.config.mjs"
      ],
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
